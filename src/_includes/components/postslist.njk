<ul>
  {% for post in postslist %}
    <li>
      <strong>
        <a href="{{ post.url | url }}">{{ post.data.title }}</a>
      </strong>
      -
      <time datetime="{{ post.date | readableDate }}">{{ post.date | readableDate }}</time>
      -
      <span>{{ post.data.description }}</span>
        <div>{{ post.data.synopsis | safe }}</div>
    </li>
  {% endfor %}
</ul>
